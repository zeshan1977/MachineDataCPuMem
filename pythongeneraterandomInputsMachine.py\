import random

def generateIncDecMixListRandomList(dLO,dHI,dNumofItems,sDirection):
    randListMix=random.randrange(dLO,dHI,dNumofItems)
    randListInc=sorted(randListMix)
    randListDec=sorted(randListMix,reverse=true)
